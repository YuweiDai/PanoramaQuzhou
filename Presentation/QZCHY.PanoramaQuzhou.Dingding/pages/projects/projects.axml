<view class="page">
  <view class="p-md">
    <scroll-view class="projects" scroll-y="true">
      <block a:for="{{projects}}">
        <view class="project" data-pid="{{item.id}}" onTap="navTo">
          <view class="logo">
            <image src="{{item.logoUrl}}" mode="scaleToFill" />
          </view>
          <view class="title">{{index+1}}.{{item.name}}
          </view>
        </view>
      </block>
    </scroll-view>
  </view>
</view>